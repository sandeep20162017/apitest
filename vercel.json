{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "./(.*)",
      "dest": "app.py"
    }
  ],
  "projectSettings": {
    "buildCommand": "pip install -r requirements.txt && gunicorn app:app",
    "outputDirectory": "./"
  }
}

